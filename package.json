{
  "name": "roman-numeral-converter",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "api project that converts roman numerals to integers and vice versa",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "check": "biome check .",
    "check:fix": "biome check --write .",
    "type-check": "tsc --noEmit",
    "qa:precommit": "npm run check:fix && npm run type-check && npm run test:coverage",
    "qa:prepush": "npm run test:ci && npm run build",
    "prepare": "husky install",
    "usage": "node .cursor/scripts/usage_report.js",
    "processes": "node .cursor/scripts/process_monitor.js",
    "dashboard": "node .cursor/scripts/dashboard_launcher.js --html",
    "build:agent-registry": "node .cursor/scripts/build-agent-registry.js"
  },
  "keywords": [
    "api",
    "ts",
    "agent-context",
    "ai-assisted"
  ],
  "author": "Gabriel Osemberg",
  "license": "MIT",
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.554.0",
    "next": "^14.2.33",
    "next-themes": "^0.4.6",
    "ps-list": "^8.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "uuid": "^9.0.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.6",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.9.1",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "@types/supertest": "^6.0.3",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "lint-staged": "^16.2.6",
    "supertest": "^7.1.4",
    "tailwindcss": "^4.1.17",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx,json}": [
      "biome check --write --no-errors-on-unmatched"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}

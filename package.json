{
  "name": "roman-numeral-converter",
  "version": "1.0.0",
  "description": "api project that converts roman numerals to integers and vice versa",
  "main": "index.js",
  "scripts": {
    "dev": "vercel dev",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.{ts,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,json,md}\"",
    "type-check": "tsc --noEmit",
    "qa:precommit": "npm run type-check && npm run lint && npm run test:coverage",
    "qa:prepush": "npm run test:ci && npm run build",
    "prepare": "husky install",
    "usage": "node .cursor/scripts/usage_report.js",
    "processes": "node .cursor/scripts/process_monitor.js",
    "dashboard": "node .cursor/scripts/dashboard_launcher.js --html",
    "build:agent-registry": "node .cursor/scripts/build-agent-registry.js"
  },
  "keywords": [
    "api",
    "ts",
    "agent-context",
    "ai-assisted"
  ],
  "author": "Gabriel Osemberg",
  "license": "MIT",
  "dependencies": {
    "ps-list": "^8.1.1",
    "uuid": "^9.0.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.9.1",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
